<act:Exports xmlns='http://schemas.microsoft.com/winfx/2006/xaml/presentation'
             xmlns:x='http://schemas.microsoft.com/winfx/2006/xaml'
             xmlns:act='clr-namespace:Microsoft.Intellipad.Composition;assembly=Microsoft.Intellipad.Framework'
             xmlns:mvstc='clr-namespace:Microsoft.VisualStudio.Text.Classification;assembly=Microsoft.VisualStudio.Platform.Editor'
             xmlns:ls='clr-namespace:Microsoft.Intellipad.LanguageServices;assembly=Microsoft.Intellipad.Core'
             >
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='Keyword' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='Identifier' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='Whitespace' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='Comment' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='Operator' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='Delimiter' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='Literal' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='String' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='Character' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='Number' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='Unknown' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
      <ls:ClassificationType Name='StaleText' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='Hyperlink' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='line number' DerivesFrom='text' />
  </act:Export>

  <!-- *********** Paragraph Formatting *************** -->
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='heading1' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='heading2' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='heading3' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='heading4' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='paragraph' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='code' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='link' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='subtle' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='subtleEmphasis' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='normal' DerivesFrom='text' />
  </act:Export>

  <!-- *********** Classifications for the Intellipad Primer *************** -->
  
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='ipadPrimerTitle' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='ipadPrimerDateText' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='ipadPrimerNormal' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='ipadPrimerHeading1' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='ipadPrimerHeading2' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='ipadPrimerCode' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='ipadPrimerBoldCode' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='ipadPrimerSubtleCode' DerivesFrom='text' />
  </act:Export>
  <act:Export Type='mvstc:ClassificationTypeDefinition'>
    <ls:ClassificationType Name='ipadPrimerMicroHeading' DerivesFrom='text' />
  </act:Export>
</act:Exports>
